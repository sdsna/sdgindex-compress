{
  "name": "@sdgindex/compress",
  "version": "0.0.2",
  "description": "Compress and decompress objects and arrays",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "trash cjs/* && cross-env NODE_ENV=cjs babel src --out-dir cjs",
    "build:es": "trash es/* && cross-env NODE_ENV=es babel src --out-dir es",
    "postbuild": "npm run postbuild:cjs && npm run postbuild:es",
    "postbuild:cjs": "trash cjs/version.js && genversion cjs/version.js",
    "postbuild:es": "trash es/version.js && genversion es/version.js",
    "test": "jest --config=jest.config.js",
    "test:production": "jest --config=jest.production.config.js",
    "generate-docs": "patch-package && jsdoc --configure jsdoc.config.js --verbose",
    "prepare": "npm run build && husky install",
    "release": "np"
  },
  "keywords": [
    "compress",
    "decompress",
    "data"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "SDG Index & Data Team",
  "license": "ISC",
  "files": [
    "cjs",
    "es"
  ],
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.isobjectlike": "^4.0.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.zipobject": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-jest": "^27.2.1",
    "clean-jsdoc-theme": "^3.2.8",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.2",
    "genversion": "^3.0.1",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.1",
    "lint-staged": "^11.1.2",
    "np": "^7.5.0",
    "patch-package": "^6.4.7",
    "prettier": "2.4.1",
    "trash-cli": "^4.0.0"
  }
}
